<UserControl x:Class="PMpanel.wpf.Views.ViewTemplates.HmoduleDashBoard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PMpanel.wpf.Views.ViewTemplates"
             mc:Ignorable="d" 
             d:DesignHeight="900" 
             d:DesignWidth="1200"
             Margin="20">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/XamlDictionary/Core.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    
    
    
    
    <StackPanel
        x:Name="Stackpanel"
        Background="{StaticResource ContainerBackground}"
        Orientation="Vertical"
        >
        <!--List of Hmodule linked to the machine line-->
    </StackPanel>
</UserControl>
